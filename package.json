{
  "name": "single-spa-mixins",
  "version": "1.0.0",
  "description": "微前端架构，内含各种轮子",
  "main": "index.js",
  "scripts": {
    "install:all": "concurrently \"npm run install:root\" \"npm run install:app1\" \"npm run install:app2\" \"npm run install:navbar\"",
    "install:root": "cd root && npm install",
    "install:app1": "cd app1 && npm install",
    "install:app2": "cd app2 && npm install",
    "install:navbar": "cd navbar && npm install",
    "dev:all": "concurrently \"npm run dev:root\" \"npm run dev:app1\" \"npm run dev:app2\" \"npm run dev:navbar\"",
    "dev:root": "cd root && npm run serve",
    "dev:app1": "cd app1 && npm run serve",
    "dev:app2": "cd app2 && npm run serve",
    "dev:navbar": "cd navbar && npm run serve",
    "audit:all": "concurrently \"npm run audit:root\" \"npm run audit:app1\" \"npm run audit:app2\" \"npm run audit:navbar\"",
    "audit:root": "cd root && npm audit fix",
    "audit:app1": "cd app1 && npm audit fix",
    "audit:app2": "cd app2 && npm audit fix",
    "audit:navbar": "cd navbar && npm audit fix",
    "build:all": "concurrently \"npm run build:app1\" \"npm run build:app2\" \"npm run build:navbar\"",
    "build:app1": "cd app1 && npm run build",
    "build:app2": "cd app2 && npm run build",
    "build:navbar": "cd navbar && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhongmeizhi/single-spa-mixins"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhongmeizhi/single-spa-mixins/issues"
  },
  "homepage": "https://github.com/zhongmeizhi/single-spa-mixins#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "express": "^4.17.1",
    "css-loader": "1.0.0",
    "style-loader": "0.23.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
